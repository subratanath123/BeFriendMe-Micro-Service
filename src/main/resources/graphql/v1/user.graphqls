extend type Query {
    friendLust: [User]
    user(id: String): User
}

extend type User {
    gender: String
    designation: String
    hasBusiness: Boolean
    aboutUser: String
    isPrivateProfile: Boolean
    coverPhotoUrl: String
    addressList: [Address]
    interestList: [Interest]
    currentLocation: [Point]
}

type Interest {
    id: String
    name: String
    imageUrl: Int
}


